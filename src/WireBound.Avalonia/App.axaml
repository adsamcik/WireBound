<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="WireBound.Avalonia.App"
             RequestedThemeVariant="Dark">
    
    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="avares://WireBound.Avalonia/Styles/Styles.axaml" />
    </Application.Styles>
    
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="avares://WireBound.Avalonia/Styles/Colors.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
    
</Application>
